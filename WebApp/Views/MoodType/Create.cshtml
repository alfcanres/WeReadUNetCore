@model MoodTypeCreateDTO

@{
    ViewData["Title"] = "Mood Type";
}

<partial name="_ValidationSummary" model=@ViewData />
}

<form asp-action="Create" method="post">



    <div class="form-group">
        <label asp-for="Mood">Mood</label>
        <input asp-for="Mood" class="form-control" />
        <span asp-validation-for="Mood" class="text-danger"></span>
    </div>

    <div class="form-group">
        <div class="form-check">
            <input asp-for="IsAvailable" class="form-check-input" type="checkbox" />
            <label asp-for="IsAvailable" class="form-check-label"></label>
        </div>
        <span asp-validation-for="IsAvailable" class="text-danger"></span>
    </div>


    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-controller="MoodType" asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
