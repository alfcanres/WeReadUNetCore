@model MoodTypeReadDTO

@{
    ViewData["Title"] = "Mood Type";
}

<partial name="_ValidationSummary" model=@ViewData />

<div class="form-group">
    <label asp-for="Id">Id</label>
    <input asp-for="Id" class="form-control" disabled="disabled" />
    <span asp-validation-for="Id" class="text-danger"></span>
</div>


<div class="form-group">
    <label asp-for="Mood">Mood</label>
    <input asp-for="Mood" class="form-control" disabled="disabled" />
    <span asp-validation-for="Mood" class="text-danger"></span>
</div>

<div class="form-group">
    <div class="form-check">
        <input asp-for="IsAvailable" class="form-check-input" type="checkbox" disabled="disabled" />
        <label asp-for="IsAvailable" class="form-check-label"></label>
    </div>
    <span asp-validation-for="IsAvailable" class="text-danger"></span>
</div>



<a asp-controller="MoodType" asp-action="Index" class="btn btn-secondary">Go back to list</a>
